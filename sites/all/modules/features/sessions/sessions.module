    'template'  => 'views-view-unformatted--sessions-schedule',
    'base hook' => 'views_view_unformatted',
    'variables' => array('view' => NULL, 'options' => NULL, 'rows' => NULL, 'title' => NULL),
    'path' => drupal_get_path('module', 'sessions').'/templates'
  );  
  return $items;
}